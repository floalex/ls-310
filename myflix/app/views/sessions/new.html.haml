%section.sign_in.container
  .row
    .col-sm-6.col-sm-offset-3
      = bootstrap_form_tag(url: sign_in_path) do |f|
        %header
          %h1 Sign In
        %fieldset         
          = f.email_field :email, label: 'Email Address'
          = f.password_field :password
          = f.form_group do
            = f.submit 'Sign In'